{
  "cosmos-installer": {
    "form": [
      {
        "name": "adminOnly",
        "label": "Do you want to make this service admin only?",
        "initialValue": true,
        "type": "checkbox"
      }
    ]
  },
  "minVersion": "0.8.0",
  "services": {
    "{ServiceName}": {
      "image": "docker:latest",
      "container_name": "{ServiceName}",
      "entrypoint": "/bin/sleep",
      "command": "infinity",
      "restart": "unless-stopped",
      "privileged": true,
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "PUID=1000",
        "PGID=1000",
        "TZ=auto"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://eralumin.github.io/cosmos-servapps/servapps/Docker/icon.png"
      },
      "volumes": [
        {
          "source": "/var/run/docker.sock",
          "target": "/var/run/docker.sock",
          "type": "bind"
        }
      ]
    }
  }
}